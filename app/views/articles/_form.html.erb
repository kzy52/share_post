<%= form_with(model: @article) do |form| %>
  <%= error_messages(@article) %>

  <div>
    <%= form.label :title %>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>

  <div>
    <%= form.label :thumbnail %>
    <%= form.file_field :thumbnail %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
